<% if user_signed_in? %>
  <%= form_with(url: search_path, method: :get) do |form| %>
    <%= form.text_field :keyword, placeholder: "キーワードを入力", value: params[:keyword] %>
    <%= form.collection_select :search_target, @search_options, :itself, :itself,  include_blank: false %>
    <%= form.select :method, ['完全一致','前方一致', '後方一致', '部分一致'], include_blank: false %>
    <%= form.submit "検索" %>
  <% end %>
<% end %>