<div class='container'>
  <div class='row'>
    <div class='col-12 col-md-8'>
      <h4>会員一覧</h4>
      <table class='table'>
        <thead>
          <tr>
            <th>会員ID</td>
            <th>名前</th>
            <!--<th>ニックネーム</th>-->
            <!--<th>メールアドレス</th>-->
            <th>ステータス</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <td><%= user.id %></td>
              <td><%= user.name %></td>
              <!--<td>< %= user.nickname %></td>-->
              <!--<td>< %= user.email %></td>-->
              <td><%= user.user_status %></td>
              <td><%= link_to "詳細表示", admin_user_path(user), class: "btn btn-success" %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>