<table class="table">
  <tbody>
    <% post.comments.each do |comment| %>
    <tr>
      <td>
        <%= link_to admin_user_path(comment.user) do %>
          <%= comment.user.nickname %>
        <% end %>
      </td>
      <td><%= comment.comment %></td>
      <td></td>
    </tr>
    <% end %>
  </tbody>
</table>